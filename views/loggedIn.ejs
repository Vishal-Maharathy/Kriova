  </body>
  <script>
    <%if(flash.success && flash.success.length>0){%>
        new Noty({
            text: '<%=flash.success%>',
            theme: 'sunset',
            type: 'success',
            layout: 'topRight',
            timeout: 1500,
        }).show();
    <%}%>
</script>
